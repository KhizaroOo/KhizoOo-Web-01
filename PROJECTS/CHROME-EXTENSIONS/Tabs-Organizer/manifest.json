{
  "manifest_version": 2,
  "name": "Tab Keeper",
  "version": "1.0",
  "description": "Organize your tabs with ease.",
  "permissions": ["tabs"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_title": "Tabs Organizer",
    "default_popup": "popup.html"
  }
}
